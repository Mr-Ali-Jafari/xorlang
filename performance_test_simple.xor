// XorLang v2.0.0 Performance Test (Simple Version)
print("=== XorLang v2.0.0 Performance Test ===");

// Test 1: Variable access performance
print("Test 1: Variable access performance");
var start_time = time_now();
var i = 0;
while (i < 10000) {
    var test_var = i;
    var result = test_var + 1;
    i = i + 1;
}
var end_time = time_now();
print("Variable access test completed in ");
print(end_time - start_time);
print(" seconds");

// Test 2: Function call performance
print("\nTest 2: Function call performance");
func test_function(x) {
    return x * 2;
}

start_time = time_now();
i = 0;
while (i < 10000) {
    var result = test_function(i);
    i = i + 1;
}
end_time = time_now();
print("Function call test completed in ");
print(end_time - start_time);
print(" seconds");

// Test 3: Array operations performance
print("\nTest 3: Array operations performance");
var array = new Array();
start_time = time_now();
i = 0;
while (i < 1000) {
    array.push(i);
    var value = array.get(i);
    i = i + 1;
}
end_time = time_now();
print("Array operations test completed in ");
print(end_time - start_time);
print(" seconds");

// Test 4: Math operations performance
print("\nTest 4: Math operations performance");
start_time = time_now();
i = 0;
while (i < 1000) {
    var sin_val = __math_sin(i);
    var cos_val = __math_cos(i);
    var sqrt_val = __math_sqrt(i + 1);
    i = i + 1;
}
end_time = time_now();
print("Math operations test completed in ");
print(end_time - start_time);
print(" seconds");

// Test 5: Class instantiation and method calls
print("\nTest 5: Class instantiation and method calls");
class TestClass {
    func init(value) {
        this.value = value;
    }
    
    func getValue() {
        return this.value;
    }
    
    func setValue(new_value) {
        this.value = new_value;
    }
}

start_time = time_now();
i = 0;
while (i < 1000) {
    var obj = new TestClass(i);
    var value = obj.getValue();
    obj.setValue(value + 1);
    i = i + 1;
}
end_time = time_now();
print("Class operations test completed in ");
print(end_time - start_time);
print(" seconds");

// Test 6: String operations performance
print("\nTest 6: String operations performance");
var test_string = "Hello, XorLang v2.0.0!";
start_time = time_now();
i = 0;
while (i < 1000) {
    var length = len(test_string);
    var char = __str_get__(test_string, i % length);
    i = i + 1;
}
end_time = time_now();
print("String operations test completed in ");
print(end_time - start_time);
print(" seconds");

print("\n=== Performance Test Completed ===");
print("XorLang v2.0.0 is optimized for speed!");
